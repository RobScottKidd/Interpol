<?xml version="1.0"?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform"
      xsi:schemaLocation="http://www.nlog-project.org/schemas/NLog.xsd NLog.xsd">
  <targets>
    <target name="file"
            fileName="C:/Logs/IntegrationHub/Interpol/CMH.CS.ERP.IntegrationHub.Interpol.ConsoleApp.log"
            archiveFileName="C:/Logs/IntegrationHub/Interpol/CMH.CS.ERP.IntegrationHub.Interpol.ConsoleApp{##}-${shortdate}.log"
            archiveAboveSize="2000000"
            xdt:Locator="Match(name)"
            xdt:Transform="SetAttributes(fileName, archiveFileName)"/>
    <target name="Tokens"
            fileName="C:/Logs/IntegrationHub/Interpol/CMH.CS.ERP.IntegrationHub.Interpol.ConsoleApp.Tokens.log"
            archiveFileName="C:/Logs/IntegrationHub/Interpol/CMH.CS.ERP.IntegrationHub.Interpol.ConsoleApp.TokenArchive{##}-${shortdate}.log"
            archiveAboveSize="2000000"
            xdt:Locator="Match(name)"
            xdt:Transform="SetAttributes(fileName, archiveFileName)"/>
  </targets>
</nlog>